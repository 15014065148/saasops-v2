CREATE TABLE `t_opt_adv_banner` (
  `id` INT(11) NOT NULL AUTO_INCREMENT COMMENT '主键id',
  `evebNum` INT(11) DEFAULT NULL COMMENT '模板Id, 1-12值',
  `clientShow` INT(11) DEFAULT NULL COMMENT '展示客户端（0：pc，1：移动，2：移动pc均显示）',
  `advType` INT(11) DEFAULT NULL COMMENT '广告类型: 1：首页，2：真人，3：电子，4：体育，5：彩票，6： 手机',
  `picPcPath` VARCHAR(256) DEFAULT NULL COMMENT 'pc广告路径',
  `picMbPath` VARCHAR(256) DEFAULT NULL COMMENT '移动广告路径',
  PRIMARY KEY (`id`)
) ENGINE=INNODB AUTO_INCREMENT=87 DEFAULT CHARSET=utf8 COMMENT='banner表';



