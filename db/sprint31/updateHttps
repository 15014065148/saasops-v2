UPDATE t_bs_bank SET backBankImg=REPLACE(backBankImg,'img-ybh.oduosa.com','download.vebapp.net');
UPDATE t_game_logo SET mbPicUrl=REPLACE(mbPicUrl,'img-ybh.oduosa.com','download.vebapp.net');
UPDATE t_game_logo SET picUrl=REPLACE(picUrl,'img-ybh.oduosa.com','download.vebapp.net');
UPDATE t_game_logo SET appPicUrl=REPLACE(appPicUrl,'img-ybh.oduosa.com','download.vebapp.net');
UPDATE t_game_logo SET logoPc=REPLACE(logoPc,'img-ybh.oduosa.com','download.vebapp.net');
UPDATE t_game_logo SET logoApp=REPLACE(logoApp,'img-ybh.oduosa.com','download.vebapp.net');
UPDATE t_game_logo SET logoMb=REPLACE(logoMb,'img-ybh.oduosa.com','download.vebapp.net');
UPDATE t_gm_game SET logo=REPLACE(logo,'img-ybh.oduosa.com','download.vebapp.net');
UPDATE t_gm_game SET logo2=REPLACE(logo2,'img-ybh.oduosa.com','download.vebapp.net');
UPDATE t_op_pay SET mBankLog=REPLACE(mBankLog,'img-ybh.oduosa.com','download.vebapp.net');
UPDATE t_opr_adv SET picMbPath=REPLACE(picMbPath,'img-ybh.oduosa.com','download.vebapp.net');
UPDATE t_opr_adv SET picPcPath=REPLACE(picPcPath,'img-ybh.oduosa.com','download.vebapp.net');
UPDATE t_opt_adv_banner SET picPcPath=REPLACE(picPcPath,'img-ybh.oduosa.com','download.vebapp.net');
UPDATE t_opt_adv_banner SET picMbPath=REPLACE(picMbPath,'img-ybh.oduosa.com','download.vebapp.net');

